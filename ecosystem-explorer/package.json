    "@testing-library/react": "16.3.2",
    "@types/node": "24.10.11",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react-swc": "4.2.3",
    "autoprefixer": "10.4.24",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react-hooks": "7.0.1",
